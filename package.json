{
  "name": "@a-novel/package-authenticator",
  "private": false,
  "version": "1.5.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "lint:eslint": "pnpm dlx eslint .",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "format": "prettier --write . --config prettier.config.ts",
    "build": "rm -rf ./dist && vite build && tsc --build tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "vitest",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "publish:patch": "pnpm version patch && git push --follow-tags",
    "publish:minor": "pnpm version minor && git push --follow-tags",
    "publish:major": "pnpm version major && git push --follow-tags"
  },
  "license": "AGPL-3.0",
  "packageManager": "pnpm@10.12.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/package-authenticator.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=10.9.2",
    "node": ">=23.11.0"
  },
  "peerDependencies": {
    "@a-novel/connector-authentication": "^1.3.0",
    "@a-novel/neon-ui": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@tanstack/react-query": "^5.75.4",
    "@tolgee/format-icu": "^6.2.6",
    "@tolgee/react": "^6.2.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.12.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@storybook/addon-docs": "9.1.0-alpha.6",
    "@storybook/addon-onboarding": "9.1.0-alpha.6",
    "@storybook/react-vite": "9.1.0-alpha.6",
    "@svgr/rollup": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.1.0-alpha.6",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nock": "^14.0.5",
    "prettier": "^3.6.2",
    "storybook": "9.1.0-alpha.6",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@a-novel/connector-authentication",
      "@a-novel/neon-ui",
      "esbuild"
    ]
  }
}
