{
  "name": "@a-novel/package-authenticator",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "lint:eslint": "pnpm dlx eslint .",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "format": "prettier --write . --config prettier.config.ts",
    "build": "rm -rf ./dist && vite build && tsc --build tsconfig.build.json",
    "test": "vitest",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "build-storybook": "storybook build"
  },
  "license": "AGPL-3.0",
  "packageManager": "pnpm@10.11.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/package-authenticator.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=10.9.2",
    "node": ">=23.11.0"
  },
  "peerDependencies": {
    "@a-novel/connector-authentication": "^1.1.0",
    "@a-novel/neon-ui": "^2.4.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@tanstack/react-query": "^5.75.4",
    "i18next": "^25.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.11.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@svgr/rollup": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitest/coverage-v8": "3.1.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "nock": "^14.0.4",
    "prettier": "^3.5.3",
    "storybook": "^8.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "yaml": "^2.8.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@a-novel/connector-authentication",
      "@a-novel/neon-ui",
      "esbuild"
    ]
  }
}
